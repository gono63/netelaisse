--
-- Name: rank_exact_matches(double precision); Type: FUNCTION; Schema: public; Owner: gono63
--

CREATE FUNCTION rank_exact_matches(rank double precision) RETURNS double precision
    LANGUAGE plpgsql
    AS $$
begin
	IF rank = 0 THEN
		return 1;
	ELSE
		return rank;
	END IF;

end
$$;


